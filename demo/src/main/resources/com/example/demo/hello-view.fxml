<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="generatePdfButton" onAction="#onGeneratePdfButtonClick" text="Générer PDF"/>

    <TableView fx:id="tableView" layoutX="14" layoutY="14" prefHeight="200" prefWidth="300">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="75" text="ID" />
            <TableColumn fx:id="nomColumn" prefWidth="100" text="Nom" />
        </columns>
    </TableView>

</VBox>

